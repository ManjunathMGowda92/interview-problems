<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Interview Problems Suite">
	<test name="Interview Problems - Numbers" parallel="classes" thread-count="4">
		<classes>
			<class name="org.manjunath.test.interviewproblems.numbers.TestArmstrongNumber"/>
			<class name="org.manjunath.test.interviewproblems.numbers.TestExtractLargestNumberFromString"/>
			<class name="org.manjunath.test.interviewproblems.numbers.TestFactorialOfNumber"/>
			<class name="org.manjunath.test.interviewproblems.numbers.TestGCDOfTwoNumbers"/>
			
			<class name="org.manjunath.test.interviewproblems.numbers.TestIntegerToBinaryConversion"/>
			<class name="org.manjunath.test.interviewproblems.numbers.TestMaxTripletSum"/>
			<class name="org.manjunath.test.interviewproblems.numbers.TestPerfectNumber"/>
			<class name="org.manjunath.test.interviewproblems.numbers.TestPrimeNumber1"/>
			
			<class name="org.manjunath.test.interviewproblems.numbers.TestPrimeNumber2"/>
			<class name="org.manjunath.test.interviewproblems.numbers.TestPrimeNumber3"/>
			<class name="org.manjunath.test.interviewproblems.numbers.TestReverseOfNumber"/>
			<class name="org.manjunath.test.interviewproblems.numbers.TestPalindromeOfNumber"/>
		</classes>
	</test>
	
	<test name="Interview Problems - Strings" parallel="classes" thread-count="4">
		<classes>
			<class name="org.manjunath.test.interviewproblems.strings.TestOccurenceOfCharsInString"/>
			<class name="org.manjunath.test.interviewproblems.strings.TestPalindromeOfString"/>
			<class name="org.manjunath.test.interviewproblems.strings.TestPalindromeOfString2"/>
			<class name="org.manjunath.test.interviewproblems.strings.TestSortStringsByLength"/>
			
			<class name="org.manjunath.test.interviewproblems.strings.TestStringAnagram1"/>
			<class name="org.manjunath.test.interviewproblems.strings.TestStringAnagram2"/>
			<class name="org.manjunath.test.interviewproblems.strings.TestStringAnagram3"/>
			<class name="org.manjunath.test.interviewproblems.strings.TestStringInplaceReverse"/>
			
			<class name="org.manjunath.test.interviewproblems.strings.TestStringRotationOfOther"/>
			<class name="org.manjunath.test.interviewproblems.strings.TestReverseOfSentence"/>
			<class name="org.manjunath.test.interviewproblems.strings.TestReverseOfSentenceUsingCollections"/>
			<class name="org.manjunath.test.interviewproblems.strings.TestStringAnagramByMergeSort"/>
			
			<class name="org.manjunath.test.interviewproblems.strings.TestFirstNonRepeatedCharFromStringUsingLinkedHashSet"/>
			<class name="org.manjunath.test.interviewproblems.strings.TestStringAnagramUsingQuickSort"/>
		</classes>
	</test>
</suite>